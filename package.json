{
  "name": "queso",
  "displayName": "Queso | Affineur apps",
  "version": "0.0.8",
  "author": "amber williams <https://github.com/amber-williams>",
  "description": "Queso - a browser extension that works within the Affienur apps ecosystem.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "react",
    "vite",
    "create-chrome-ext"
  ],
  "engines": {
    "node": ">=18.19.0"
  },
  "homepage": "https://github.com/amber-williams",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:zip": "tsc && vite build --mode example",
    "preview": "vite preview",
    "fmt": "prettier --write '**/*.{tsx,ts,json,css,scss,md}'",
    "zip": "pnpm run build:zip && node src/zip.js"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@mb3r/component-library": "1.4.1"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.19",
    "@types/chrome": "0.0.268",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.2.13",
    "@vitejs/plugin-react": "4.3.1",
    "gulp": "5.0.0",
    "gulp-zip": "6.0.0",
    "prettier": "3.3.2",
    "typescript": "5.2.2",
    "vite": "5.3.1"
  }
}
